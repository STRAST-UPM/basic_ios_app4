# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

target 'basic_app' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for basic_app
  pod 'Capacitor'
  # Facebook SDK
  pod 'FBSDKCoreKit'
  pod 'FBSDKCoreKit_Basics'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  pod 'FBAEMKit'
  # Firebase SDK
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Crashlytics'
  pod 'Firebase/Analytics'

  target 'basic_appTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'basic_appUITests' do
    # Pods for testing
  end

end
